Boolean verif(Grille g) {

        int i;
        int count=0;

        for(i=0; i!=HAUTEUR; i++) {
                 while(g[i][count]!='\0'){
                        count++;
                }

        if(count!=LARGEUR){
                error(MAUVAISE_LARG);
                return false;

        }
        }

        return true;

}

